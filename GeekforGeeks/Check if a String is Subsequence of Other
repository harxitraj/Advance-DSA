class Solution {
    public boolean isSubSeq(String s1, String s2) {
        // code here
        int m = s1.length();
        int n = s2.length();
        int i = 0;
        int j = 0;
        while(i < m && j < n) {
            if(s1.charAt(i) == s2.charAt(j)) {
                i++;
            }
            j++;
        }
        if(i == m) {
            return true;
        } else {
            return false;
        }
    }
}
