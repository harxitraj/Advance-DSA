class Solution {
    // Function to delete middle element of a stack.
    public void deleteMid(Stack<Integer> s) {
        // code here
        int size = s.size();
        int mid = (size/2) + 1;
        solve(s, mid);
    }
    public void solve(Stack<Integer> s, int k) {
        if(k == 1) {
            s.pop();
            return;
        }
        int top = s.pop();
        solve(s, k-1);
        s.push(top);
    }
}
