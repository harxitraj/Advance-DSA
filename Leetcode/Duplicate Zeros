class Solution {
    public void duplicateZeros(int[] arr) {
        int n = arr.length;
        int x = arr[0];
        int y = arr[n-1];
        for(int i = 0; i < n; i++) {
            if(arr[i] == 0) {
                for(int j = n-1; j > i; j--) {
                    arr[j] = arr[j-1];
                }
                i++;
            }
        }
    }
}
