class Solution {
    public List<Integer> largestDivisibleSubset(int[] nums) {
        int n = nums.length;
        ArrayList<Integer> ans = new ArrayList<>();
        int[] dp = new int[n];
        int[] parent = new int[n];
        Arrays.fill(dp, 1);
        Arrays.fill(parent, -1);
        Arrays.sort(nums);
        int max = 1;
        int lastIndex = 0;
        for(int curr = 1; curr < n; curr++) {
            for(int prev = 0; prev < curr; prev++) {
                if(nums[curr] % nums[prev] == 0 && dp[prev]+1 > dp[curr]) {
                    dp[curr] = dp[prev] + 1;
                    max = Math.max(max, dp[curr]);
                    parent[curr] = prev;

                }
            }
        }
        for(int i = 0; i < n; i++) {
            if(dp[i] == max) {
                lastIndex = i;
                break;
            }
        }
        for(int i = lastIndex; i != -1; i = parent[i]) {
            ans.add(nums[i]);
        }
        Collections.reverse(ans);
        return ans;
    }
}
