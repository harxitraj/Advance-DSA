class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        HashSet<Integer> set = new HashSet<>();
        ArrayList<Integer> res = new ArrayList<>();
        for (int num : nums) {
            if (set.contains(num)){
                res.add(num);
            }else {
                set.add(num);
            }
        }
        int[] ans = new int[2];
        ans[0] = res.get(0);
        ans[1] = res.get(1);
        return ans;
    }
}
